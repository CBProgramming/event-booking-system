@model IEnumerable<ThAmCo.Events.Services.MenuDto>


<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            <td>
                @Html.ActionLink("Edit", "Edit", new { item.MenuId }) |
                @Html.ActionLink("Details", "Details", new { item.MenuId }) |
                @Html.ActionLink("Delete", "Delete", new { item.MenuId })
            </td>
        </tr>
}
    </tbody>
</table>
